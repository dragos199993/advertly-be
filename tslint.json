{
  "rules": {
    "class-name": true,
    "curly": true,
    "eofline": true,
    "indent": [
      true,
      2
    ],
    "max-line-length": [
      true,
      130
    ],
    "member-ordering": [
      "static-before-instance",
      "variables-before-functions"
    ],
    "no-arg": true,
    "no-consecutive-blank-lines": true,
    "no-console": [
      false
    ],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-unused-variables": false,
    "no-empty": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "trailing-comma": "never",
    "no-trailing-whitespace": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": true,
    "no-var-keyword": true,
    "one-line": [
      "check-catch",
      "check-else",
      "check-open-brace",
      "check-whitespace"
    ],
    "radix": true,
    "semicolon": true,
    "switch-default": true,
    "typedef": [
      "call-signature",
      "parameter",
      "property-declaration",
      "variable-declaration",
      "member-variable-declaration"
    ],
    "whitespace": [
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  }
}
